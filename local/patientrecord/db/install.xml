    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="patient_complete_details" COMMENT="Defines patient_complete_details">
      <FIELDS>

        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="feedback_id" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="completed_id" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="consoler_username" TYPE="char" LENGTH="256" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="paadharnumber" TYPE="char" LENGTH="256" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
         <FIELD NAME="pfname" TYPE="char" LENGTH="256" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
          <FIELD NAME="plname" TYPE="char" LENGTH="256" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="pcity" TYPE="char" LENGTH="256" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="patient_id" TYPE="char" LENGTH="256" NOTNULL="true" SEQUENCE="false"/>
         <FIELD NAME="status" TYPE="char" LENGTH="5" NOTNULL="true" SEQUENCE="false"/>
     
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="completed_id" UNIQUE="false" FIELDS="completed_id"/>
      </INDEXES>
    </TABLE>

